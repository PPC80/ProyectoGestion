            <Button fx:id="btnModificar" layoutX="14.0" layoutY="100.0" mnemonicParsing="false" onMouseClicked="#modificar" prefHeight="38.0" prefWidth="135.0" text="Modificar proveedor" />
         </children>
      </Pane>
      <Button fx:id="btnAtras" layoutX="499.0" layoutY="31.0" mnemonicParsing="false" onMouseClicked="#atras" prefHeight="26.0" prefWidth="66.0" style="-fx-background-color: Aquamarine;" text="Atras" />
   </children>
</AnchorPane>
